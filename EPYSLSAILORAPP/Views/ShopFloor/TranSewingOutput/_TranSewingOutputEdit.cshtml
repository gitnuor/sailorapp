@using EPYSLSAILORAPP.Domain.DTO;
@using EPYSLSAILORAPP.Domain.Entity;
@using EPYSLSAILORAPP.Domain.RPC;
@using Newtonsoft.Json;
@model EPYSLSAILORAPP.Domain.DTO.tran_sewing_output_DTO;
@{
    ViewBag.Title = "TranSewingOutput Edit";
    Layout = null;
}
<style>
    #toolbarId select {
        font-size: 11px !important;
    }

    .btninnerClose {
        color: #a52711;
        width: 26px;
        border: 1px solid #6f2d23;
        font-size: 22px;
        margin-top: 6px;
    }

    .dataTables_wrapper {
        margin-top: -40px;
    }

    .select_style {
        height: 30px;
    }

    .dataTable select {
        height: 25px !important;
        font-size: 10px !important;
    }

    .card-body {
        padding-bottom: 0px;
        padding-top: 0px;
        padding: 0rem 0rem;
    }

    .box {
        margin-bottom: 0.5rem;
    }

    .box-header {
        padding-top: 0.25rem;
        padding-bottom: 0.25rem;
        font-weight: 500;
        font-size: 12px;
    }

    .box-title {
        font-size: 12px;
        line-height: 1rem;
    }
</style>



<form method="POST" id="frmEdit" class="my-login-validation" novalidate="" enctype="multipart/form-data" autocomplete="off">

    
    <input type="hidden" id="tran_sewing_output_id" value="@Model.tran_sewing_output_id">

  

     <div class="card">
         <div class="card-header">
               <h6 class="card-title">TranSewingOutput Edit</h6> 
         </div>
         <!-- /.card-header -->
         <div class="card-body">
        

                 		<div class="row" >
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Tran Sewing Input Id</label>
					 <input style="width:100%"    asp-for="tran_sewing_input_id" type="number" class="border-#d1cccc-200 form-control " />
					<span asp-validation-for="tran_sewing_input_id" class="text-danger"></span>
				</div>
			</div>
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Tran Sewing Allocation Plan Id</label>
					<input style="width:100%"    asp-for="tran_sewing_allocation_plan_id" type="number" class="border-#d1cccc-200 form-control " />
				<span asp-validation-for="tran_sewing_allocation_plan_id" class="text-danger"></span>
			</div>
		</div>
		</div>

		<div class="row" >
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Techpack Id</label>
					 <input style="width:100%"    asp-for="techpack_id" type="number" class="border-#d1cccc-200 form-control " />
					<span asp-validation-for="techpack_id" class="text-danger"></span>
				</div>
			</div>
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Output Date</label>
					<input style="width:100%"    asp-for="output_date" type="date" class="border-#d1cccc-200 form-control " />
				<span asp-validation-for="output_date" class="text-danger"></span>
			</div>
		</div>
		</div>

		<div class="row" >
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Note</label>
					 <input style="width:100%"    asp-for="note" type="text" class="border-#d1cccc-200 form-control " />
					<span asp-validation-for="note" class="text-danger"></span>
				</div>
			</div>
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Hour Output</label>
					<input style="width:100%"    asp-for="hour_output" type="text" class="border-#d1cccc-200 form-control " />
				<span asp-validation-for="hour_output" class="text-danger"></span>
			</div>
		</div>
		</div>

		<div class="row" >
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Fiscal Year Id</label>
					 <input style="width:100%"    asp-for="fiscal_year_id" type="number" class="border-#d1cccc-200 form-control " />
					<span asp-validation-for="fiscal_year_id" class="text-danger"></span>
				</div>
			</div>
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Event Id</label>
					<input style="width:100%"    asp-for="event_id" type="number" class="border-#d1cccc-200 form-control " />
				<span asp-validation-for="event_id" class="text-danger"></span>
			</div>
		</div>
		</div>

		<div class="row" >
			<div class="col-md-6">
				<div class="form-group">
					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Tran Sewing Output Details</label>
					 <input style="width:100%"    asp-for="tran_sewing_output_details" type="text" class="border-#d1cccc-200 form-control " />
					<span asp-validation-for="tran_sewing_output_details" class="text-danger"></span>
				</div>
			</div>

		</div>
		</div>



           
             <div class="row">
                <div class="col-md-12" style="margin-bottom: 5px;text-align:right;">

                    <button style="width:300px;" type="button" class="btn btn-success " id="btnUpdateTranSewingOutput" onclick="UpdateTranSewingOutput(this);"><i class="fa fa-file-open"></i>&nbsp;Update</button>

                </div>
            </div>

         </div>
         <!-- /.card-body -->
     </div>


    
</form>





