        function SaveFinishingProduction(btn) {
            debugger;
            //alert('okk');

            // if (!_cusFormValidate("frmAddReceive")) {
            //    return;
            // }

            details = []

            var ItemCategory = $('#ItemCategory').val();
            var style_item_product_id = $('#style_item_product_id').val();
            var FinishingReceiveDate = $('#FinishingReceiveDate').val();
            var techpackid = $('#techpack_id').val();
            var note = $('#note').val();
            var FinishingProcessName = $('#FinishingProcessName').val();
            var FinishingProcessId = $('#FinishingProcessId').val();

            var currentDateTime = new Date();
            var productionProcessDate = formatDate(currentDateTime);
            function formatDate(date) {
                return [
                    date.getFullYear(),
                    padTo2Digits(date.getMonth() + 1),
                    padTo2Digits(date.getDate()),
                ].join('-') + ' ' + [
                    padTo2Digits(date.getHours()),
                    padTo2Digits(date.getMinutes()),
                    padTo2Digits(date.getSeconds()),
                ].join(':');
            }
            function padTo2Digits(num) {
                return num.toString().padStart(2, '0');
            }

            $.each($("#DTProductionBatchDetailReceive tbody tr"), function (key, val) {

                var obj = {

                    color_code: $(val).find(".color_code").val(),
                    //size_name: $(val).find(".size_name").val(),
                    // color_quantity: $(val).find(".colorQty").val(),
                    finish_receive_qty: $(val).find(".finish_receive_qty").val(),
                    production_quantity: $(val).find(".rec_qty").val(),
                    bal_qty: $(val).find(".rec_bal_qty").val()
                    // style_item_product_id: $(val).find(".partId").val()
                };
                details.push(obj);

            });

            var obj_emb_finishing_production = {
                techpack_id: techpackid,
                style_item_product_id: style_item_product_id,
                tran_finish_receive_date: FinishingReceiveDate,
                style_item_product_category: ItemCategory,
                note: note,
                finishing_process_name: FinishingProcessName,
                gen_finishing_process_id: FinishingProcessId,
                //production_process_date: productionProcessDate,

                is_submitted: 1,

                details: details
            }

            setTimeout(function () {
                showLoader("Saving..........");
            }, 0);

            ajaxPostObjectHandler("/FinishingProductionProcess/SaveFinishingProduction", obj_emb_finishing_production, function (response) {

                setTimeout(function () {
                    hideLoader();
                }, 0);

                if (response.status_Code == "200") {



                    // LoadFinishingReceivePendingData(techpackid);


                    // LoadReceiveChallanDraftData();
                    // LoadReceiveChallanApprovedData();
                    // LoadReceiveChallanProposedData();

                    showSuccessAlert("Success", response.status_Result, "OK", function () {
                        TranFinishingProductionView(techpackid);

                        closePopupLanding();

                    });
                }
                else {
                    showErrorAlert("Alert", response.status_Result, "OK", function () {

                    });
                }
            }, true, function () { hideLoader(); }, true);

        }


