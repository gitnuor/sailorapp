<form method="POST" id="frmView" class="my-login-validation" novalidate="" enctype="multipart/form-data" autocomplete="off">

      <input type="hidden" id="chat_group_id" value="@Model.chat_group_id">

           <div class="card">
             <div class="card-header">
                   <h6 class="card-title">GenChatGroup View</h6> 
             </div>
       
             <div class="card-body">
               		<div class="row" >
			<div class="col-md-12">
				<div class="form-group">
   					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Chat Group Name</label>
   					<input style="width:100%" disabled  asp-for="chat_group_name" type="text" class="border-#d1cccc-200 form-control" />
    					<span asp-validation-for="chat_group_name" class="text-danger"></span>
				</div>
			</div>
		</div>

		<div class="row" >
			<div class="col-md-12">
				<div class="form-group">
   					<label class="labelNormal " style="margin-bottom: 10px!important;font-weight:bold;">Group Users</label>
   					<input style="width:100%" disabled  asp-for="group_users" type="text" class="border-#d1cccc-200 form-control" />
    					<span asp-validation-for="group_users" class="text-danger"></span>
				</div>
			</div>
		</div>


             </div>
          </div>
	 
	
  

</form>





