<script>
    var obj_fabric_List = [];
    function AddNewFabric(btn) {

        $("#modalcontainer_common").addClass("custom-class");
        var item_structure_group_id = $("#genItenStrucId").val();
        var gen_item_structure_group_sub_id = $("#ddlSubCategory").val();

        try {
            ajaxGetHandler("/FabricRequisition/AddNewFabric?item_structure_group_id=" + item_structure_group_id + "&gen_item_structure_group_sub_id=" + gen_item_structure_group_sub_id + "&measurement_unit_id=" + 7, null, function (data) {
                hideLoader();
                $('#modalcontent-common_popup').html(data);
                $('#modalcontainer_common_popup').modal({ backdrop: 'static', keyboard: false });
